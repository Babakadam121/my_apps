<div class="main_form">
<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <div class="field">
          <%= label_tag :email %><br />
          <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
          <%= label_tag :password %><br />
          <%= password_field_tag :password %>
  </div>
  <div class="actions sub_mit"><%= submit_tag "Log In" %>
<% end %>
</div>
